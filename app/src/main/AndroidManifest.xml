<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.example.hatewait">

    <uses-permission android:name="android.permission.INTERNET " />
    <uses-permission android:name="android.permission.VIBRATE " />
    <uses-permission android:name="android.permission.WAKE_LOCK " />

    <application
            android:allowBackup="true"
            android:icon="@drawable/app_logo"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".StoreInfoUpdate"></activity>
        <!-- <activity android:name=".customer_menu"> -->
        <!-- </activity> -->
        <!-- <activity android:name=".store_menu"> -->
        <!-- </activity> -->
        <!--  -->
        <activity android:name=".LoginRegisterViewPagerActivity"></activity> <!--  -->
        <activity android:name=".Register_Check"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>